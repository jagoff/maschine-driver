CXX = g++
CXXFLAGS = -std=c++11 -framework CoreMIDI -framework CoreFoundation -framework IOKit
TARGET = debug_midi_devices
SOURCE = debug_midi_devices.cpp

all: $(TARGET)

$(TARGET): $(SOURCE)
	$(CXX) $(CXXFLAGS) -o $(TARGET) $(SOURCE)

clean:
	rm -f $(TARGET)

run: $(TARGET)
	./$(TARGET)

.PHONY: all clean run 